# ── Modelo de Embeddings ──────────────────────────
EMBEDDER_MODEL=nomic-embed-text
EMBEDDER_DIMENSIONS=768

# ── Banco Vetorial (LanceDB) ─────────────────────
VECTOR_DB_TABLE=docs
VECTOR_DB_URI=tmp/lancedb

# ── LLM (Ollama) ─────────────────────────────────
LLM_MODEL=llama3.1
OLLAMA_HOST=localhost
OLLAMA_PORT=11434

# ── Embedder (Ollama) ────────────────────────────
EMBEDDER_HOST=localhost
EMBEDDER_PORT=11435

# ── Memória / Histórico ──────────────────────────
MEMORY_DB_FILE=tmp/agent.db
MEMORY_HISTORY_RUNS=3

# ── Funcionalidades ──────────────────────────────
ENABLE_WEB_SEARCH=true
MARKDOWN=true

# ── Configurações Adicionais Agno ─────────────────────
# Debug mode (true/false)
DEBUG_MODE=false
